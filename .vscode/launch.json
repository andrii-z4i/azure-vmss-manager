    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "--resource-group=dev-wallet", 
                "--vmss-name=dev-wallet-vmss",
                "--vault-name=dev-wallet-kv",
                "--secret-name=dev-wallet-vmss-ssh"
            ],
            "justMyCode": true
        }
    ]
}